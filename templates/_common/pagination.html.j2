<nav aria-label="Page navigation example">
  <ul class="pagination">
    
    <li class="page-item disabled">
      <a class="page-link" href="#" aria-label="Previous">
        <span aria-hidden="true">&laquo;</span>
        <span class="sr-only">Previous</span>
      </a>
    </li>

    {% for current_page in range(total_pages) %}
    <li class="page-item {% if current_page+1 == page %} active {% endif %}">
      {% if current_page+1 == page %}
        <li class="page-item active" aria-current="page">
          <span class="page-link">{{ current_page+1 }}</span>
        </li>          
      {% else %}
        <a class="page-link" data-turbo-frame="test_frame" href="/test?template={{ template }}&page={{ current_page+1 }}">{{ current_page+1 }}</a>
      {% endif%}
    </li>
    {% endfor %}
    
    <li class="page-item disabled">
      <a class="page-link" href="#" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
        <span class="sr-only">Next</span>
      </a>
    </li>

  </ul>
</nav>
